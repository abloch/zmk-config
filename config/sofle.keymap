#include <dt-bindings/zmk/mouse.h>
#include <behaviors/mouse_keys.dtsi>
#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/pointing.h>

&sl { release-after-ms = <60000>; };

&msc {
    acceleration-exponent = <2>;
    time-to-max-speed-ms = <1500>;
    delay-ms = <0>;
};

&mmv {
    time-to-max-speed-ms = <1500>;
    acceleration-exponent = <2>;
    trigger-period-ms = <16>;
};

/ {
    scroll_encoder: scroll_encoder {
        compatible = "zmk,behavior-sensor-rotate";
        #sensor-binding-cells = <0>;
        bindings = <&msc SCRL_DOWN>, <&msc SCRL_UP>;

        tap-ms = <100>;
    };

    macros {
        // M0: {} with cursor inside

        m0: m0 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LEFT_BRACE &kp RBRC &kp LEFT_ARROW>;
            label = "M0";
        };

        // M1: () with cursor inside  

        m1: m1 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LEFT_PARENTHESIS &kp RIGHT_PARENTHESIS &kp LEFT_ARROW>;
            label = "M1";
        };

        // M2: [] with cursor inside

        m2: m2 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LEFT_BRACKET &kp RIGHT_BRACKET &kp LEFT_ARROW>;
            label = "M2";
        };

        // M3: Terminal command

        m3: m3 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LG(LC(LA(T)))>,
                <&macro_wait_time 100>,
                <&kp LA(NUMBER_3)>;

            label = "M3";
        };

        // M4: Alt+7, Cmd+Enter

        m4: m4 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LA(NUMBER_7)>,
                <&macro_wait_time 100>,
                <&kp LG(ENTER)>;

            label = "M4";
        };

        // M5: Cmd+K, Cmd+[

        m5: m5 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(K) &kp LG(LEFT_BRACKET)>;
            label = "M5";
        };

        // M6: Cmd+K, Cmd+]

        m6: m6 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(K) &kp LG(RIGHT_BRACKET)>;
            label = "M6";
        };

        // M7: Cmd+K, Cmd+0

        m7: m7 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(K) &kp LG(NUMBER_0)>;
            label = "M7";
        };

        // M8: Alt+Cmd+P

        m8: m8 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&kp LALT &kp LGUI>,
                <&macro_tap>,
                <&kp P>,
                <&macro_release>,
                <&kp LALT &kp LGUI>;

            label = "M8";
        };

        // M9: ESC, ZZ (vim exit)

        m9: m9 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp ESC &kp LS(Z) &kp LS(Z)>;
            label = "M9";
        };

        // M10: Ctrl+Cmd+Q

        m10: m10 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(LG(Q))>;
            label = "M10";
        };

        // M11: Alt+1, Ctrl+1

        m11: m11 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LA(NUMBER_1)>,
                <&macro_wait_time 100>,
                <&kp LC(NUMBER_1)>;

            label = "M11";
        };

        // M12: Alt+1, Ctrl+2

        m12: m12 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LA(NUMBER_1)>,
                <&macro_wait_time 100>,
                <&kp LC(NUMBER_2)>;

            label = "M12";
        };

        // M13: Alt+Ctrl+Left

        m13: m13 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LA(LC(LEFT_ARROW))>;
            label = "M13";
        };

        // M14: Alt+Ctrl+Right

        m14: m14 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LA(LC(RIGHT_ARROW))>;
            label = "M14";
        };

        // M15: "kubiya" text

        m15: m15 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&macro_tap>, <&kp K &kp U &kp B &kp I &kp Y &kp A>;

            label = "M15";
        };

        // M20: Ctrl+`, !!, Enter

        m20: m20 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LC(GRAVE)>,
                <&macro_wait_time 100>,
                <&kp EXCL &kp EXCL &kp ENTER>;

            label = "M20";
        };

        // Legacy macros from original keymap

        arc1: arc1 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LA(NUMBER_1)>,
                <&macro_wait_time 100>,
                <&kp LC(NUMBER_1)>;

            label = "ARC1";
        };

        arc2: arc2 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LA(NUMBER_1)>,
                <&macro_wait_time 100>,
                <&kp LC(NUMBER_2)>;

            label = "ARC2";
        };

        parent: parent {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LEFT_PARENTHESIS &kp RIGHT_PARENTHESIS &kp LEFT_ARROW>;
            label = "PARENT";
        };

        brackets: brackets {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LEFT_BRACKET &kp RIGHT_BRACKET &kp LEFT_ARROW>;
            label = "BRACKETS";
        };

        term: term {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LG(LC(LA(T)))>,
                <&macro_wait_time 30>,
                <&kp LA(NUMBER_3)>;

            label = "TERM";
        };

        foldall: foldall {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(K) &kp LG(NUMBER_0)>;
            label = "FOLDALL";
        };

        fold: fold {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(K) &kp LG(LEFT_BRACKET)>;
            label = "FOLD";
        };

        expand: expand {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(K) &kp LG(RIGHT_BRACKET)>;
            label = "EXPAND";
        };

        exitvi: exitvi {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp ESC &kp LS(Z) &kp LS(Z)>;
            label = "EXITVI";
        };

        thisisunsafe: thisisunsafe {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp T &kp H &kp I &kp S &kp I &kp S &kp U &kp N &kp S &kp A &kp F &kp E>;
            label = "THISISUNSAFE";
        };

        bruno: bruno {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LA(NUMBER_7)>;
            label = "BRUNO";
        };
    };

    behaviors {
        td10: tap_dance_10 {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&mkp MB5>, <&none>, <&m10>;
        };

        gui5: gui5 {
            compatible = "zmk,behavior-tap-dance";
            label = "GUI5";
            #binding-cells = <0>;
            bindings = <&kp LEFT_GUI>, <&kp LEFT_GUI>, <&to 5>;

            tapping-term-ms = <400>;
        };

        alt5: alt5 {
            compatible = "zmk,behavior-tap-dance";
            label = "ALT5";
            #binding-cells = <0>;
            bindings = <&kp LEFT_ALT>, <&kp LEFT_ALT>, <&to 5>;

            tapping-term-ms = <400>;
        };
    };

    combos {
        compatible = "zmk,combos";
        timeout-ms = <150>;

        // Adapted combos for Sofle 60-key layout
        // Sofle key positions (main keys, excluding encoders):
        // Row 0:  0   1   2   3   4   5       7   8   9  10  11  12
        // Row 1: 13  14  15  16  17  18      20  21  22  23  24  25
        // Row 2: 26  27  28  29  30  31      33  34  35  36  37  38
        // Row 3: 39  40  41  42  43  44      46  47  48  49  50  51
        // Row 4: 52  53  54  55  56  57      59  60  61  62  63  64

        semicolon {
            bindings = <&kp SEMI>;
            key-positions = <31 18>; // R + F (Corne 5 17 → Sofle 17 30)
        };

        pipe {
            bindings = <&kp PIPE>;
            key-positions = <31 44>; // G + B (Corne 17 29 → Sofle 30 43)
        };

        slash {
            bindings = <&kp SLASH>;
            key-positions = <29 17>; // S + E (Corne 15 4 → Sofle 28 16)
        };

        backslash {
            bindings = <&kp BACKSLASH>;
            key-positions = <16 30>; // D + E (Corne 3 16 → Sofle 16 29)
        };

        dash {
            bindings = <&kp MINUS>;
            key-positions = <28 30>; // D + S (Corne 14 16 → Sofle 28 29)
        };

        underscore {
            bindings = <&kp UNDER>;
            key-positions = <41 43>; // X + V (Corne 26 28 → Sofle 41 43)
        };

        plus {
            bindings = <&kp PLUS>;
            key-positions = <34 36>; // J + L (Corne 19 21 → Sofle 34 36)
        };

        eql {
            bindings = <&kp EQUAL>;
            key-positions = <47 49>; // M + . (Corne 31 33 → Sofle 47 49)
        };

        mo3 {
            bindings = <&mo 3>;
            key-positions = <28 29 30>; // S + D + F (Corne 14 15 16 → Sofle 28 29 30)
        };

        mo4_right {
            bindings = <&sl 4>;
            key-positions = <34 35 36>; // J + K + L (Corne 19 20 21 → Sofle 34 35 36)
        };

        mo2 {
            bindings = <&mo 2>;
            key-positions = <28 16 30>; // S + E + F (Corne 14 3 16 → Sofle 28 16 30)
        };

        to2 {
            bindings = <&to 2>;
            key-positions = <27 28 16 30>; // A + S + E + F (Corne 13 14 3 16 → Sofle 27 28 16 30)
        };

        to3 {
            bindings = <&to 3>;
            key-positions = <27 28 29 30>; // A + S + D + F (Corne 13 14 15 16 → Sofle 27 28 29 30)
        };

        mouse1 {
            bindings = <&mkp LCLK>;
            key-positions = <13 26>; // W + D (Corne 0 12 → Sofle 13 26)
        };

        mouse2 {
            bindings = <&mkp RCLK>;
            key-positions = <26 39>; // A + C (Corne 12 24 → Sofle 26 39)
        };

        mo4_right2 {
            bindings = <&sl 4>;
            key-positions = <47 48 49>; // M + , + . (Corne 31 32 33 → Sofle 47 48 49)
        };

        bt0 {
            bindings = <&bt BT_SEL 0>;
            key-positions = <13 25>;
        };

        bt1 {
            bindings = <&bt BT_SEL 1>;
            key-positions = <14 25>;
        };

        bt2 {
            bindings = <&bt BT_SEL 2>;
            key-positions = <15 25>;
        };

        disconnect0 {
            bindings = <&bt BT_DISC 0>;
            key-positions = <26 25>;
        };

        disconnect1 {
            bindings = <&bt BT_DISC 1>;
            key-positions = <27 25>;
        };

        disconnect2 {
            bindings = <&bt BT_DISC 2>;
            key-positions = <28 25>;
        };

        clearall {
            bindings = <&bt BT_CLR_ALL>;
            key-positions = <13 26 39 25>;
        };

        disconnect_bt {
            bindings = <&bt BT_CLR>;
            key-positions = <13 26 25>;
        };

        left_brackets {
            bindings = <&kp LEFT_BRACKET>;
            key-positions = <26 27>;
        };

        right_brackets {
            bindings = <&kp RIGHT_BRACKET>;
            key-positions = <27 28>;
        };
    };

    keymap {
        compatible = "zmk,keymap";

        // Layer 0: Base layer adapted for Sofle from Corne
        // ┌───┬───┬───┬───┬───┬───┐     ┌───┬───┬───┬───┬───┬───┐
        // │BLD│ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │     │ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │BLD│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │TAB│ Q │ W │ E │ R │ T │     │ Y │ U │ I │ O │ P │TD10│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │ESC│ A │ S │ D │ F │ G │     │ H │ J │ K │ L │ ⌫ │MB4│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │ ` │ Z │ X │ C │ V │ B │     │ N │ M │ , │ . │ / │ ~ │
        // └───┴───┴───┼───┼───┼───┤     ├───┼───┼───┼───┴───┴───┘
        //             │GUI5│L2-SP│ ⇧ │     │ ⌃ │L1-⏎│ALT5│
        //             └───┴───┴───┘     └───┴───┴───┘

        base {
            bindings = <
&bootloader  &none  &none  &none  &none        &none        &none  &none      &none        &none      &none    &none      &bootloader
&kp TAB      &kp Q  &kp W  &kp E  &kp R        &kp T        &none  &kp Y      &kp U        &kp I      &kp O    &kp P      &td10
&kp ESCAPE   &kp A  &kp S  &kp D  &kp F        &kp G        &none  &kp H      &kp J        &kp K      &kp L    &kp BSPC   &mkp MB4
&kp GRAVE    &kp Z  &kp X  &kp C  &kp V        &kp B        &none  &kp N      &kp M        &kp COMMA  &kp DOT  &kp SLASH  &kp TILDE
&none        &none  &none  &gui5  &lt 2 SPACE  &kp LSHFT    &none  &kp LCTRL  &lt 1 ENTER  &alt5      &none    &none
            >;

            sensor-bindings = <&inc_dec_kp LG(LS(Z)) LG(Z)>;
            display-name = "base";
        };

        // Layer 1: Symbols layer adapted for Sofle from Corne
        // ┌───┬───┬───┬───┬───┬───┐     ┌───┬───┬───┬───┬───┬───┐
        // │BLD│ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │     │ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │BLD│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │TO0│{ }│ : │ " │ ' │ $ │     │ ^ │⌥ 7│⌥ 8│⌥ 9│M11│M12│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │ESC│( )│ $ │ % │ & │ * │     │TRM│⌥ 4│⌥ 5│⌥ 6│⌥⌘←│⌘⌥→│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │ ? │[ ]│ ! │ @ │ # │ : │     │⌥ 0│⌥ 1│⌥ 2│⌥ 3│⌥⌃←│⌥⌃→│
        // └───┴───┴───┼───┼───┼───┤     ├───┼───┼───┼───┴───┴───┘
        //             │ ⌘ │SPC│ ⇧ │     │ ⌃ │ ⏎ │ ⌥ │
        //             └───┴───┴───┘     └───┴───┴───┘

        symbols {
            bindings = <
&bootloader  &none  &none            &none        &none          &none           &none  &none       &none       &none       &none       &none             &bootloader
&to 0        &m0    &kp COLON        &kp DQT      &kp SQT        &kp DOLLAR      &none  &kp CARET   &kp LA(N7)  &kp LA(N8)  &kp LA(N9)  &m11              &m12
&kp ESC      &m1    &kp DOLLAR       &kp PERCENT  &kp AMPERSAND  &kp ASTERISK    &none  &term       &kp LA(N4)  &kp LA(N5)  &kp LA(N6)  &kp LA(LG(LEFT))  &kp LG(LA(RIGHT))
&kp QMARK    &m2    &kp EXCLAMATION  &kp AT       &kp HASH       &kp COLON       &none  &kp LA(N0)  &kp LA(N1)  &kp LA(N2)  &kp LA(N3)  &kp LA(LC(LEFT))  &kp LA(LC(RIGHT))
&none        &none  &none            &kp LGUI     &kp SPACE      &kp LSHFT       &none  &kp LCTRL   &kp RET     &kp LALT    &none       &none
            >;

            display-name = "symbols";
            sensor-bindings = <&inc_dec_kp LG(RIGHT) LG(LEFT)>;
        };

        // Layer 2: Navigation layer adapted for Sofle from Corne
        // ┌───┬───┬───┬───┬───┬───┐     ┌───┬───┬───┬───┬───┬───┐
        // │BLD│ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │     │ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │BLD│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │TO0│⌘ 1│⌘ 2│⌘ 3│SC→│SC→│     │FD*│HOM│ ▲ │PgU│   │TO0│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │ESC│⌃ 1│⌃ 2│⌃ 3│⌃ 4│⌃ 5│     │FLD│ ◄ │ ▼ │ ► │⌥ ⌫│SC↑│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │⌃ `│⌘⇧E│⌥⌘T│   │SF4│ F4│     │EXP│END│DEL│PgD│⌥ ⌦│SC↓│
        // └───┴───┴───┼───┼───┼───┤     ├───┼───┼───┼───┴───┴───┘
        //             │ ⌘ │SPC│ ⇧ │     │ ⌃ │ ⏎ │ ⌥ │
        //             └───┴───┴───┘     └───┴───┴───┘

        nav {
            bindings = <
&bootloader    &none          &none          &none       &none            &none              &none  &none             &none     &none     &none      &none         &bootloader
&to 0          &kp LG(N1)     &kp LG(N2)     &kp LG(N3)  &msc SCRL_RIGHT  &msc SCRL_RIGHT    &none  &foldall          &kp HOME  &kp UP    &kp PG_UP  &none         &to 0
&kp ESC        &kp LC(N1)     &kp LC(N2)     &kp LC(N3)  &kp LC(N4)       &kp LC(N5)         &none  &fold             &kp LEFT  &kp DOWN  &kp RIGHT  &kp LA(BSPC)  &msc SCRL_UP
&kp LC(GRAVE)  &kp LG(LS(E))  &kp LA(LG(T))  &none       &kp LS(F4)       &kp F4             &none  &expand           &kp END   &kp DEL   &kp PG_DN  &kp LA(DEL)   &msc SCRL_DOWN
&none          &none          &none          &kp LGUI    &kp SPACE        &kp LSHFT          &none  &kp LEFT_CONTROL  &kp RET   &kp LALT  &none      &none
            >;

            sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN>;
            display-name = "nav";
        };

        // Layer 3: Numbers layer adapted for Sofle from Corne
        // ┌───┬───┬───┬───┬───┬───┐     ┌───┬───┬───┬───┬───┬───┐
        // │BLD│ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │     │ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │BLD│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │TO0│ 1 │ 2 │ 3 │ 4 │ 5 │     │ / │ 7 │ 8 │ 9 │ * │TO0│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │TO0│⌃ 1│⌃ 2│⌃ 3│⌃ 4│⌃ 5│     │ + │ 4 │ 5 │ 6 │ ⌫ │   │
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │   │   │   │   │   │   │     │ - │ 1 │ 2 │ 3 │ _ │ . │
        // └───┴───┴───┼───┼───┼───┤     ├───┼───┼───┼───┴───┴───┘
        //             │ ⌘ │SPC│ ⇧ │     │ . │ 0 │ ⌥ │
        //             └───┴───┴───┘     └───┴───┴───┘

        numbers {
            bindings = <
&bootloader  &none       &none       &none       &none       &none               &none  &none      &none   &none     &none   &none         &bootloader
&to 0        &kp N1      &kp N2      &kp N3      &kp N4      &kp N5              &none  &kp SLASH  &kp N7  &kp N8    &kp N9  &kp ASTERISK  &to 0
&to 0        &kp LC(N1)  &kp LC(N2)  &kp LC(N3)  &kp LC(N4)  &kp LC(NUMBER_5)    &none  &kp PLUS   &kp N4  &kp N5    &kp N6  &kp BSPC      &none
&none        &none       &none       &none       &none       &none               &none  &kp MINUS  &kp N1  &kp N2    &kp N3  &kp UNDER     &kp DOT
&none        &none       &none       &kp LGUI    &kp SPACE   &kp LSHFT           &none  &kp DOT    &kp N0  &kp LALT  &none   &none
            >;

            sensor-bindings = <&scroll_encoder>;
            display-name = "numbers";
        };

        // Layer 4: Function layer adapted for Sofle from Corne
        // ┌───┬───┬───┬───┬───┬───┐     ┌───┬───┬───┬───┬───┬───┐
        // │BLD│ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │     │ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │BLD│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │TO0│M9 │ F2│⌘F12│F12│ F9│     │ F5│F10│F11│SF11│M8 │TO0│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │TO0│M20│CAG│⇧⌘5│⌘ F│⇧⌘F│     │⇧⌘F5│F4│M15│⇧⌘L│⌘F8│   │
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │CAP│   │⌘⇧4│⇧⌘C│⌘ V│CAV│     │⇧ F5│M4│SAFE│TO5│⌥⌃←│⌥⌃→│
        // └───┴───┴───┼───┼───┼───┤     ├───┼───┼───┼───┴───┴───┘
        //             │ ⌘ │⌘⇧ESC│CPW│     │ ⌃ │⇧⌘SP│⌥ │
        //             └───┴───┴───┘     └───┴───┴───┘

        function {
            bindings = <
&bootloader  &none          &none                 &none           &none               &none                &none  &none           &none              &none          &none          &none             &bootloader
&to 0        &m9            &kp F2                &kp LG(F12)     &kp F12             &kp F9               &none  &kp F5          &kp F10            &kp F11        &kp LS(F11)    &m8               &to 0
&to 0        &m20           &kp LC(LA(LG(S)))     &kp LS(LG(N5))  &kp LG(F)           &kp LS(LG(F))        &none  &kp LS(LG(F5))  &kp F4             &m15           &kp LS(LG(L))  &kp LG(F8)        &kp LC(LG(LA(BACKSLASH)))
&kp CAPS     &thisisunsafe  &kp LG(LS(NUMBER_4))  &kp LS(LG(C))   &kp LG(V)           &kp LC(LA(LG(V)))    &none  &kp LS(F5)      &m4                &thisisunsafe  &to 5          &kp LC(LA(LEFT))  &kp LA(LC(RIGHT))
&none        &none          &none                 &kp LGUI        &kp LG(LS(ESCAPE))  &caps_word           &none  &kp LCTRL       &kp LS(LG(SPACE))  &kp LALT       &none          &none
            >;

            sensor-bindings = <&scroll_encoder>;
            display-name = "function";
        };

        // Layer 5: Bluetooth & Mouse layer adapted for Sofle from Corne
        // ┌───┬───┬───┬───┬───┬───┐     ┌───┬───┬───┬───┬───┬───┐
        // │BLD│ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │     │ ∅ │ ∅ │ ∅ │ ∅ │ ∅ │BLD│
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │BT0│BT1│BT2│BT3│BT4│CLR│     │CLR│ F7│ F8│ F9│M↑ │S↑ │
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │DC0│DC1│DC2│DC3│DC4│C-A│     │C-A│ F4│ F5│ F6│M← │M→ │
        // ├───┼───┼───┼───┼───┼───┤     ├───┼───┼───┼───┼───┼───┤
        // │   │SAFE│  │   │   │BLD│     │BLD│ F1│ F2│ F3│M↓ │S↓ │
        // └───┴───┴───┼───┼───┼───┤     ├───┼───┼───┼───┴───┴───┘
        //             │ ⌘ │TO0│ ⇧ │     │ ⌃ │TO0│ ⌥ │
        //             └───┴───┴───┘     └───┴───┴───┘

        bt {
            bindings = <
&bootloader    &none          &none          &none          &none          &none             &none  &none             &none   &none     &none   &none           &bootloader
&bt BT_SEL 0   &bt BT_SEL 1   &bt BT_SEL 2   &bt BT_SEL 3   &bt BT_SEL 4   &bt BT_CLR        &none  &bt BT_CLR        &kp F7  &kp F8    &kp F9  &mmv MOVE_UP    &msc SCRL_UP
&bt BT_DISC 0  &bt BT_DISC 1  &bt BT_DISC 2  &bt BT_DISC 3  &bt BT_DISC 4  &bt BT_CLR_ALL    &none  &bt BT_CLR_ALL    &kp F4  &kp F5    &kp F6  &mmv MOVE_LEFT  &mmv MOVE_RIGHT
&none          &thisisunsafe  &none          &none          &none          &bootloader       &none  &bootloader       &kp F1  &kp F2    &kp F3  &mmv MOVE_DOWN  &msc SCRL_DOWN
&none          &none          &none          &kp LEFT_GUI   &to 0          &kp LEFT_SHIFT    &none  &kp LEFT_CONTROL  &to 0   &kp LALT  &none   &none
            >;

            sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN>;
            display-name = "bt";
        };
    };

    conditional_layers {
    };
};
